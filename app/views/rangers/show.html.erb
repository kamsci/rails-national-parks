<h1>Ranger <%= @ranger.name %></h1>
<p><%= @ranger.name %> works at the following parks:</p>

<ul>
  <% @ranger.parks.each do |park| %>
    <li><%= park.name %></li>
  <% end %>
</ul>

<ul>
  <li class="inline btn btn-primary"><%= link_to('Edit', edit_ranger_path) %></li>
  <li class="inline btn btn-danger"><%= link_to('Delete', @ranger, method: :delete) %></li>
</ul>